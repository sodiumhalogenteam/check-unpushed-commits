{
  "name": "check-unpushed-commits",
  "version": "0.2.4",
  "description": "check repos in a specified folder for commits that haven't been pushed",
  "main": "index.js",
  "bin": {
    "check-unpushed-commits": "index.js",
    "cuc": "index.js"
  },
